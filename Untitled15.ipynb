    "import time  \n",
    "def countdown(c):      \n",
    "    while c:  \n",
    "        m, s = divmod(c, 60)  \n",
    "        timer = '{:02d}:{:02d}'.format(m, s)  \n",
    "        print(timer, end=\"\\r\")  \n",
    "        time.sleep(1)  \n",
    "        c -= 1       \n",
    "    print('time is up!!')  \n",
    "c = input(\"ur timer is \")  \n",
    "countdown(int(c))  "
   ]
  }
